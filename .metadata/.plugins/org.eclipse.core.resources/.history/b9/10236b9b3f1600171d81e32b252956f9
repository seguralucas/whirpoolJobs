package exit.services.principal.automata;

import java.util.ArrayList;
import java.util.regex.Matcher;
import java.util.regex.Pattern;

public class AutomataFinito {
	
	public static void main(String[] args) {
		AutomataFinito.leer("a(\"a.b.c\", \"tex,to\")");
	}
	
	public static String leer(String cadena){
		ArrayList<String> tipos= new ArrayList<String>();
		ArrayList<String> valores= new ArrayList<String>();
		String name = "a movieaS01E02 with some stuff";
		Pattern p = Pattern.compile("(a movie)");
		Matcher m = p.matcher(name);

		if (m.find())
		    System.out.println(name.substring(m.end()));
		return null;
	}
}
